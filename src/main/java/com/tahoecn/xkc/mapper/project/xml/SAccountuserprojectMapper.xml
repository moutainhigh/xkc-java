<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tahoecn.xkc.mapper.project.SAccountuserprojectMapper">

	<!-- 用户切换项目 -->
	<update id="changeUserProject" parameterType="java.util.Map">
		UPDATE S_AccountUserProject 
    	SET ProjectID = #{ProjectID} 
    	WHERE UserID = #{UserID} 
	</update>
</mapper>
